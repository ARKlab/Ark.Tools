    <PackageVersion Include="Ark.Tools.NLog" Version="999.9.9" />
    <PackageVersion Include="Ark.Tools.Reqnroll" Version="999.9.9" />
    <PackageVersion Include="Ark.Tools.ResourceWatcher.Testing" Version="999.9.9" />
    <PackageVersion Include="Ark.Tools.ResourceWatcher.WorkerHost.Hosting" Version="999.9.9" />
  </ItemGroup>
  
  <ItemGroup>
    <!-- 3rd party packages used by this sample -->
    <PackageVersion Include="CsvHelper" Version="33.1.0" />
    
    <!-- Testing packages -->
    <PackageVersion Include="AwesomeAssertions" Version="9.3.0" />
    <PackageVersion Include="Microsoft.NET.Test.Sdk" Version="18.0.1" />
    <PackageVersion Include="Microsoft.Testing.Platform" Version="2.0.2" />
    <PackageVersion Include="Microsoft.Testing.Extensions.CrashDump" Version="2.0.2" />
    <PackageVersion Include="Microsoft.Testing.Extensions.CodeCoverage" Version="18.3.2" />
    <PackageVersion Include="Microsoft.Testing.Extensions.HangDump" Version="2.0.2" />
    <PackageVersion Include="Microsoft.Testing.Extensions.HotReload" Version="2.0.2" />
    <PackageVersion Include="Microsoft.Testing.Extensions.Retry" Version="2.0.2" />
    <PackageVersion Include="Microsoft.Testing.Extensions.TrxReport" Version="2.0.2" />
    <PackageVersion Include="Microsoft.Testing.Extensions.AzureDevOpsReport" Version="2.0.2" />
    <PackageVersion Include="MSTest.TestAdapter" Version="4.0.2" />
    <PackageVersion Include="MSTest.TestFramework" Version="4.0.2" />
    <PackageVersion Include="MSTest.Analyzers" Version="4.0.2" />
    <PackageVersion Include="Moq" Version="4.20.72" />
    <PackageVersion Include="Reqnroll.MsTest" Version="3.3.2" />
    <PackageVersion Include="Reqnroll.Tools.MsBuild.Generation" Version="3.3.2" />
    
    <!-- Build tools -->
    <PackageVersion Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="10.0.102" />
    <PackageVersion Include="Microsoft.CodeAnalysis.BannedApiAnalyzers" Version="4.14.0" />
    <PackageVersion Include="Meziantou.Analyzer" Version="2.0.277" />
