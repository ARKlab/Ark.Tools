{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>ARKlab/renovate-config"
  ],
  "constraints": {
    "dotnet": "10.0.103"
  },
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^renovate.json$/"
      ],
      "matchStrings": [
        "\"dotnet\":\\s*\"(?<currentValue>.*?)\""
      ],
      "depNameTemplate": "dotnet-sdk",
      "datasourceTemplate": "dotnet-version",
      "versioningTemplate": "semver"
    }
  ]
}
