<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
	<Description>Tools and base classes for AspNetCore Health Checks as in Ark-way of using it</Description>
	<PackageTags>Ark;C#;.NET;Framework;AspNetCore;HealthCheck</PackageTags>
	<Version>$(AspNetCoreGroupVersion)</Version>
	<VersionSuffix>$(AspNetCoreGroupVersionSuffix)</VersionSuffix>
  </PropertyGroup>

  <ItemGroup>
	  <PackageReference Include="AspNetCore.HealthChecks.AzureServiceBus" Version="3.2.2" />
	  <PackageReference Include="AspNetCore.HealthChecks.Network" Version="3.1.3" />
	  <PackageReference Include="AspNetCore.HealthChecks.Oracle" Version="3.1.2" />
	  <PackageReference Include="AspNetCore.HealthChecks.RavenDB" Version="3.1.1" />
	  <PackageReference Include="AspNetCore.HealthChecks.SqlServer" Version="3.1.1" />
	  <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks.EntityFrameworkCore" Version="3.1.8" />
  </ItemGroup>

</Project>
